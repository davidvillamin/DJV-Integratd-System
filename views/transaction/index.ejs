              <div class="card-body">
                  <h5 class="card-title">Reports <span>/Today</span></h5>

                  <!-- Line Chart -->
                  <div id="reportsChart"></div>

                  <script>
                      document.addEventListener("DOMContentLoaded", () => {
                          new ApexCharts(document.querySelector("#reportsChart"), {
                              series: [{
                                  name: 'In-House',
                                  data: [31, 40, 28, 51, 42, 82, 56],
                              }, {
                                  name: 'On-Site',
                                  data: [11, 32, 45, 32, 34, 52, 41]
                              }, {
                                  name: 'Sales',
                                  data: [15, 11, 32, 18, 9, 24, 11]
                              },
                                 {
                                  name: 'Project-based',
                                  data: [20, 31, 62, 48, 69, 74, 51]
                              }],
                              chart: {
                                  height: 350,
                                  type: 'area',
                                  toolbar: {
                                      show: false
                                  },
                              },
                              markers: {
                                  size: 2
                              },
                              colors: ['#4154f1', '#2eca6a', '#ff771d', '#f7b924', '#ff007f', '#ff00ff', '#00ff00', '#0000ff', '#ff0000', '#00ffff', '#ffff00', '#000000'],
                              fill: {
                                  type: "gradient",
                                  gradient: {
                                      shadeIntensity: 1,
                                      opacityFrom: 0.3,
                                      opacityTo: 0.4,
                                      stops: [0, 90, 100]
                                  }
                              },
                              dataLabels: {
                                  enabled: false
                              },
                              stroke: {
                                  curve: 'smooth',
                                  width: 4
                              },
                              xaxis: {
                                  type: 'date',
                                  categories: ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"],
                              },
                              tooltip: {
                                  x: {
                                      format: 'MM/yy'
                                  },
                              }
                          }).render();
                      });
                  </script>
                  <!-- End Line Chart -->

              </div>

          </div>
